<!DOCTYPE html>

<html>
<head>
  <title><%= title %></title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, target-densitydpi=160dpi, initial-scale=1.0; maximum-scale=1.0; user-scalable=0;">
</head>
<body>
  <div id="container">

    <div id="background"></div>

    <% if (sources.length > 1) { %>
      <ul id="jump_to">
        <li>
          <div id="file_section">
              <button id="filelist">All files</button>
              <button id="filechoice">Search file</button>
          </div>
          <div id="jump_menu">
            <a class="large" href="javascript:void(0);">Jump To &hellip;</a>
            <a class="small" href="javascript:void(0);">+</a>

            <div id="jump_wrapper">
              <div id="jump_choice">
                   <input id="autocompletion" placeholder="Enter file name: "/>
              </div>
              <!--<div id="jump_source" class="jump_page">
              </div>-->
              <div id="jump_source" class="jump_page">
                 <% for (var i=0, l=sources.length; i<l; i++) { %>
                 <% var source = sources[i]; %>
                 <a class="source" href="<%= path.basename(destination(source)) %>">
                    <%= path.basename(source) %>
                 </a>
                 <% } %>
              </div>
            </div>
      </div>
        </li>
      </ul>
    <% } %>
    <ul class="sections">
        <% if (!hasTitle) { %>
          <li id="title">
              <div class="annotation">
                  <h1><img id="aeicon" alt="ae-icon" src="http://nyocommercialobserver.files.wordpress.com/2013/03/ae_logo.gif"/><%= title %></h1>
              </div>
          </li>
        <% } %>
        <% for (var i=0, l=sections.length; i<l; i++) { %>
        <% var section = sections[i]; %>

        <li id="section-<%= i + 1 %>">
            <div class="annotation">
              <% heading = section.docsHtml.match(/^\s*<(h\d)>/) %>
              <div class="pilwrap <%= heading ? 'for-' + heading[1] : '' %>">
                <a class="pilcrow" href="#section-<%= i + 1 %>">&#182;Comment <%= i + 1 %>:</a>
              </div>
              <%= section.docsHtml %>
            </div>
            <% if (section.codeText.replace(/\s/gm, '') != '') { %>
            <div class="content">
              <p class="section_caption">Section <%= i + 1 %> : </p>
              <%= section.codeHtml %>
            </div>
            <% } %>
        </li>
        <% } %>
    </ul>
  </div>
  <% if (sources.length>1){ %>
     <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
     <link rel="stylesheet" media="all" href="<%= css %>" />
     <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
     <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
     <script src="jump_menu.js"></script>
  <% } %>
</body>
</html>
